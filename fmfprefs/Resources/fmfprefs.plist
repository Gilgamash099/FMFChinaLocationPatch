<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
	<key>items</key>
	<array>
		<dict>
			<key>cell</key>
			<string>PSSwitchCell</string>
			<key>default</key>
			<false/>
			<key>defaults</key>
			<string>com.weishi.fmflocationfix-prefs</string>
			<key>key</key>
			<string>enableLocationSpoofing</string>
			<key>label</key>
			<string>Enable location spoofing</string>
		</dict>

        <dict>
            <key>cell</key>
            <string>PSGroupCell</string>
            <key>label</key>
            <string>Coordinates</string>
            <key>footerText</key>
            <string>Location shift fix for mainland China is automatically applied. Use regular GPS coordinates for spoofing.</string>
        </dict>
		<dict>
			<key>cell</key>
			<string>PSEditTextCell</string>
			<key>defaults</key>
			<string>com.weishi.fmflocationfix-prefs</string>
			<key>key</key>
			<string>latitude</string>
			<key>label</key>
			<string>Latitude</string>
			<key>keyboard</key>
            <string>numbers</string>
        </dict>
		<dict>
			<key>cell</key>
			<string>PSEditTextCell</string>
			<key>defaults</key>
			<string>com.weishi.fmflocationfix-prefs</string>
			<key>key</key>
			<string>longitude</string>
			<key>label</key>
			<string>Longitude</string>
			<key>keyboard</key>
            <string>numbers</string>
        </dict>
        
        <dict>
            <key>cell</key>
            <string>PSGroupCell</string>
            <key>label</key>
            <string>About</string>
            <key>footerText</key>
            <string>Source code is available at github.com/weishi/FMFChinaLocationPatch</string>
        </dict>
        <dict>
            <key>cell</key>
            <string>PSTitleValueCell</string>
            <key>label</key>
            <string>Version</string>
            <key>get</key>
            <string>getVersion:</string>
        </dict>
        <dict>
            <key>cell</key>
            <string>PSTitleValueCell</string>
            <key>label</key>
            <string>Author</string>
            <key>get</key>
            <string>getAuthor:</string>
        </dict>
        <dict>
            <key>cell</key>
            <string>PSTitleValueCell</string>
            <key>label</key>
            <string>Twitter</string>
            <key>get</key>
            <string>getTwitter:</string>
        </dict>
        <dict>
            <key>cell</key>
            <string>PSTitleValueCell</string>
            <key>label</key>
            <string>Blog</string>
            <key>get</key>
            <string>getBlog:</string>
        </dict>
	</array>
	<key>title</key>
	<string>FMFLocationSpoofer</string>
</dict>
</plist>
